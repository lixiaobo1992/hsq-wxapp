.ali-footer{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 92rpx;
  border-top: 1rpx solid #e2e2e2;
  background-color: #fff;
}
.footer-item{
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content:center;
}
.footer-icon{
  width: 36rpx;
  height: 36rpx;
  align-self:center;
  margin-top: 10rpx;
}
.footer-text{
  align-self:center;
  margin-top: 5rpx;
  font-size: 24rpx;
}