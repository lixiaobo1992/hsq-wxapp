.labels {
  display: flex;
  height: 80rpx;
  width: 100%;
  background: white;
  position: relative;
  align-items: center;
  justify-content: space-around;
  color: #666;
  border-bottom: 2rpx solid #e2e2e2;
}

.labels .label {
  /* flex: 1; */
  display: inline-flex;
  align-items: center;
}

.label .image {
  width: 32rpx;
  height: 32rpx;
}

.label .text {
  font-size: 28rpx;
  margin-left: 8rpx;
  white-space: nowrap;
}
