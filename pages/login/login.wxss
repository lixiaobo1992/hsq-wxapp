.page-login {
  padding: 32rpx;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background-color: #fff;
  position: relative;
}
.login-center {
  text-align: center;
  margin: 200rpx 0;
}

.logo-box{
  width: 160rpx;
  height: 160rpx;
  margin: 0 auto;
}

.logo-box image{
  width: 100%;
  height: 100%;
}

.brands-tip{
  font-size: 34rpx;
  color: #212121;
  margin-top: 50rpx;
}

.page-login .loginBtn{
  position: absolute;
  left: 5%;
  bottom: 150rpx;
  width: 90%; 
  box-sizing: border-box;
  font-size: 36rpx;
  line-height: 90rpx;
}

.goHomeBtn{
  position: fixed;
  left: 32rpx;
  top: 2%;
  width: 80rpx;
  height: 80rpx;
  background-color: rgba(0,0,0,0.4);
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 100%;
}
.goHomeBtn image{
  width: 60%;
  height: 60%;
}

