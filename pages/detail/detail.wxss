} */
.member-detail .left-icon .vip-tips{
  width:32rpx;
  height:32rpx;
  display:inline-block;
  margin-left:4rpx;
}
.member-detail .btn-icon{
  margin-right:10rpx;
}
.member-detail .btn-icon text{
  color:#222222;
  font-weight:bold;
  font-size:28rpx;
}
.member-detail .btn-icon .rule-more{
  margin-left:4rpx;
  margin-top:-4rpx;
}
.member-detail .vip-icon{
  width:40rpx;
  height:40rpx;
  margin-right:12rpx;
  margin-left:14rpx;
  align-items:flex-start;
  border-radius:4rpx;
}
.member-detail .vip-text{
  color:#333;
  font-size:26rpx;
}
.member-detail .vip-text .red{
  color: rgb(237, 0, 0);
}
.member-detail .vip-tips{
  width:28rpx;
  height:28rpx;
  vertical-align: middle;
}

/* 轻会员 */
.price-box .item.item-price{
  font-size:48rpx;
}
.market-price {
  font-size: 44rpx;
  color: #F23D3D;
  font-weight: 500;
  margin-right:10rpx;
}
.market-price text{
  font-size: 60rpx;
}
.member-price-icon image{
  width: 24rpx;
  height: 24rpx;
  flex-shrink: 0;
}

.member-price-detail {
  float: left;
  font-size: 24rpx;
  color: #333333;
  padding: 0 4rpx;
}
/* 会员价 */
.price-box .item.item-rmb{
  font-size:48rpx;
  font-weight:500;
}
.member-price{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.member-price .vip-icon{
  width:28rpx;
  height:28rpx;
}
.member-price .item-member-price{
  font-size: 28rpx;
  color: #333333;
  background: #FDFBEB;
  height:28rpx;
  line-height: 28rpx;
}


