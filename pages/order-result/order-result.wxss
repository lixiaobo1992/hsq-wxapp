
.page {
  height: 100vh;
  background: #fff;
  text-align: center;
  padding: 80rpx 0 0;
}

.hidden {
  display: none;
}

.result-tit .icon {
  display: block;
  margin: 0 auto 40rpx;
  width: 240rpx;
  height: 240rpx;
}

.order-status-text {
  font-size: 32rpx;
  color: #000;
}

.price {
  margin: 20rpx 0;
  font-size: 60rpx;
  color: #000;
}

.btn-info,
.btn-sure {
  font-size: 24rpx;
  border: 2rpx solid #CDCDCD;
  border-radius: 26rpx;
  padding: 0 30rpx;
  height: 52rpx;
  line-height: 52rpx;
}
.btn-info {
  margin-left: 30rpx;
  color: rgb(167, 157, 157);
}
.btn-sure {
  color: #F73552;
  border-color: #F73552;
}

.btn-box {
  margin-top: 80rpx;
}
