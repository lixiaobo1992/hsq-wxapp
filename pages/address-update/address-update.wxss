/* pages/address-update/address-update.wxss */
.address-item{
  position: relative;
  border-bottom: 1px solid #cdcdcd;
  background-color: #ffffff;
  overflow: hidden;

  display:flex;
  width:100%;
  height:42px;
  line-height:42px;
}

.address-label{
  flex:3;
  padding-left:10px;
  min-width:100px;
}

.address-input{
  border: none;
  flex:7;
  height:100%;
  line-height:42px;
}

.address-select{
  background-image: url(data:image/png;base64,) no-repeat right center;
  background-size: 40rpx auto;
  flex:7;
  height:100%;
}

.address-add-box{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #ffffff;
  text-align: center;
  background-color: red;
  z-index: 999;
}

.address-content{
  position: fixed;
  z-index: 1000;
  width: 100%;
  left: 0;
  bottom: -280px;
  background-color: #ffffff;
  transition: 0.5s all;
}

.address-content-show{
  bottom: 0px;
}

.address-commit-box{
  position: relative;
  left: 0;
  top: 0;
  height: 40px;
  border-bottom: 1px solid #cdcdcd;
}

.address-commit{
  position: absolute;
  top: 0;
  height: 40px;
  line-height: 40px;
  padding: 0rpx 50rpx;
  color: #108EE9;
}

.address-commit-cancel{
  left: 0;
}

.address-commit-sure{
  right: 0;
}

picker-view picker-view-column view {
  text-align: center;
}