} */
.member-detail .left-icon .vip-tips{
  width:32rpx;
  height:32rpx;
  display:inline-block;
  margin-left:4rpx;
}
.member-detail .btn-icon{
  margin-right:10rpx;
}
.member-detail .btn-icon text{
  color:#222222;
  font-weight:bold;
  font-size:28rpx;
}
.member-detail .btn-icon .rule-more{
  margin-left:4rpx;
  margin-top:-4rpx;
}
.member-detail .vip-icon{
  width:40rpx;
  height:40rpx;
  margin-right:12rpx;
  margin-left:14rpx;
  align-items:flex-start;
  border-radius:4rpx;
}
.member-detail .vip-text{
  color:#333;
  font-size:26rpx;
}
.member-detail .vip-text .red{
  color: rgb(237, 0, 0);
}
.member-detail .vip-tips{
  width:28rpx;
  height:28rpx;
  vertical-align: middle;
}

/* 运费信息 */
.city-front, 
.city-cost {
  display: flex;
  align-items: center;
}
.city-img{
  flex: 0 0 auto;
  width: 24rpx;
  height: 24rpx;
  margin-right: 4rpx;
  position: relative;
  top: -2rpx;
}
.city-label{
  flex: 0 0 auto;
  font-size: 24rpx;
  color: #999999;
}
.city-cost {
  padding: 16rpx 20rpx;
  padding-left: 0;
  margin-left: 16rpx;
  border-top: 1px #E2E2E2 solid;
}
.city{
  position: relative;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 16rpx;
  padding-bottom: 16rpx;
  background-color: #fff;
}
.city-name{
  margin-left: 10rpx;
  font-size: 24rpx;
  color: black;
  margin-right: 80rpx;
  word-break: break-all;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  max-width: 100%;
}
.cannot-delivery {
  color: #F21833;
}
.city-arrow{
  position: absolute;
  right: 20rpx;
  top: 24rpx;
  width: 30rpx;
  height: 30rpx;
}

/* 会员价 */
.price-box .item.item-rmb{
  font-size:48rpx;
  font-weight:500;
}
.market-price {
  font-size: 48rpx;
  color: #F23D3D;
  font-weight: 500;
  margin-right:10rpx;
}
.market-price text{
  font-size: 60rpx;
}
.member-price{
  margin-right:10rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top:10rpx;
}
.member-price .vip-icon{
  width:28rpx;
  height:28rpx;
}
.member-price .item-member-price{
  font-size: 28rpx;
  color: #333333;
  background: #FDFBEB;
  height:28rpx;
  line-height: 28rpx;
}
