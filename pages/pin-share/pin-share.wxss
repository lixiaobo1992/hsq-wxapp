}
.jump-app-box{
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  border-radius: 1000rpx;
  position: relative;
  margin-top: 10rpx;
}
.app-icon{
  position: absolute;
  width: 80rpx;
  height: 80rpx; 
  z-index: 9;
  left: 0;
  top: 0;
}
.jump-btn{
  margin: 0;
  margin-top: 10rpx;
