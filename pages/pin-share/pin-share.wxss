.share-content{
  position: relative;
  flex: 1;
}
.share-title{
  font-size: 28rpx;
  font-weight: 700;
  line-height: 40rpx;
  margin-bottom: 20rpx;
}
.info-line{
  display: flex;
  font-size: 24rpx;
  color: #b4b4b4;
  align-items: center;
  line-height: 1.2;
  margin-bottom: 20rpx;
}
.count-limit{
  font-size: 24rpx;
  padding: 4rpx 14rpx;
  border: 2rpx solid #ED0000;
  color: #ED0000;
  margin-right: 10rpx;
  border-radius: 4rpx;
}
.couple-price{
  font-size: 28rpx;
  color: #ed0000;
  margin-right: 10rpx;
}
.del-num{
  margin-left: 6rpx;
  text-decoration: line-through;
}
.single-bug{
  font-size: 28rpx;
  color: #999999;
  line-height: 40rpx;
}
.states-icon{
  position: absolute;
  display: block;
  right: 30rpx;
  top: 104rpx;
  width: 170rpx;
  height: 120rpx;
  z-index: 2;
}
.avatar-list{
  text-align: center;
  margin-bottom: 54rpx;
  overflow: hidden;
  min-height: 50rpx;
  width: 100%;
}  
.user-avatar{
  display: inline-block;
  width: 17%;
  text-align: center;
  position: relative;
  margin-bottom: 10rpx;
}
.user-avatar:first-child::after{
  content: '团长';
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
  width: 60rpx;
  height: 30rpx;
  border-radius: 100rpx;
  background: #F5A623;
  border: 2rpx solid #F9BC01;
  font-size: 20rpx;
  line-height: 30rpx;
  text-align: center;
  color: #FFFFFF;
  letter-spacing: -0.24rpx;
}
.robot-avatar image{
  width: 102rpx;
  height: 102rpx;
  border-radius: 1000rpx;
}
.joined-avatar{
  width: 98rpx;
  height: 98rpx;
  border-radius: 1000rpx;
  border: 4rpx solid #F9BC01;
}

.user-avatar .add-icon{
  position: absolute;
  top: 0;
  right: 4rpx;
  width: 30rpx;
  height: 30rpx;
}
.more-than-5{
  text-align: left;
}
.text-btn{
  float: right;
  padding: 10rpx 30rpx;
  margin-right: 40rpx;
}
.share-btn{
  padding-bottom: 40rpx;
  height: 120rpx;
  line-height: 80rpx;
}
.share{
  width: 100%;
  height: 100%;
  background: #ED0000;
  border-radius: 6px;
  line-height: 80rpx;
  text-align: center;
  font-size: 34rpx;
  color: #FFFFFF;
  letter-spacing: -0.4px;
  border: none;
}
.fixed_footer{
  position: relative;
  width: 100%;
  text-align: center;
}
.order-buy{
  width: 100%;
  background-color: #ed0000;
  height: 80rpx;
  border-radius: 6rpx;
  text-align: center;
  font-size: 34rpx;
  line-height: 80rpx;
  color: #fff;
}
.order-detail{
  position: relative;
  width: 100%;
  height: 80rpx;
  padding: 20rpx 0rpx;
  text-align: center;
  font-size: 28rpx;
  color: #ED0000;
  letter-spacing: -0.33px;
  line-height: 40rpx;
}
.order-detail .more-icon {
  width: 14rpx;
  height: 24rpx;
  position: absolute;
  top: 50%;
  margin-top: -12rpx;
  margin-left: 6rpx;
}
.view-tip {
  display: flex;
  position: relative;
  width: 100%;
  height: 100rpx;
  background-color: #F5F5F9;
  justify-content: center;
  align-items: center;
}

.view-tip .tip-line {
  width: 100%;
  margin: 0 60rpx;
  height: 1px;
  background-color: #e2e2e2;
}

.view-tip .tip-title {
  position: absolute;
  left: 50%;
  top: 0;
  margin-left: -100rpx;
  width: 200rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  color: #999999;
  text-align: center;
  background-color: #F5F5F9;
}
.more-activity{
  position: relative;
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #212121;
  font-size: 24rpx;
}
.more-activity .more-icon{
  width: 14rpx;
  height: 24rpx;
  position: absolute;
  top: 50%;
  margin-top: -12rpx;
  margin-left: 6rpx;
}
.skus-mask{
  position: fixed;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0,0,0,.5);
  left: 0;
  top: 0;
  z-index: 1;
}
.skus-select-tip{
  position: fixed;
  bottom: -100%;
  left: 0;
  width: 100%;
  height: 80%;
  background-color: #fff;
  transition-property: bottom;
  transition-duration: 0.5s;
  transition-timing-function: ease-in-out;
  padding: 190rpx 30rpx 0;
  z-index: 201;
  padding-bottom: 98rpx;
}
.select-tip-ani{
  bottom: 0%;
}
.sku-info{
  display: flex;
  padding-bottom: 40rpx;
  position: absolute;
  top: 0;
  left: 30rpx;
  width: 100%;
  border-bottom: 1px solid #e2e2e2;
}
.sku-image{
  width: 200rpx;
  height: 200rpx;
  background-color: #fff;
  margin-top: -50rpx;
}
.sku-image image{
  width: 100%;
  height: 100%;
}
.sku-desc{
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}
.sku-price{
  color: #ed0000;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  line-height: 50rpx;
}
.sku-price text{
  user-select: none;
  font-size: 36rpx;
}
.sku-stock{
  font-size: 24rpx;
  line-height: 40rpx;
  color: #212121;
  font-family: PingFangSC-Regular;
}
.sku-number{
  font-size: 24rpx;
  color: #999;
  font-family: PingFangSC-Regular;
}

.sku-buy-btn{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  text-align: center;
  color: #fff;
  background-color: #ed0000;
  line-height: 98rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
}
.sku-attrs-list{
  height: 100%;
  overflow: scroll;
}
.sku-block-list{
  padding: 10px 0;
  border-top: 1px solid #e2e2e2;
  font-size: 24rpx;
}
.sku-block-list:first-child{
  border: none;
}
.sku-block-tit{
  font-size: 28rpx;
  margin-bottom: 20rpx;
}
.sku-block-tit text{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #ED0000;
}
.sku-item-list{
  display: flex;
  flex-wrap: wrap;
}
.sku-item{
  margin-bottom: 20rpx;
  flex-shrink: 0;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 24rpx;
  background-color: #fafaf8;
  border-radius: 100px;
  margin-right: 20rpx;
  border: 1px solid #fafaf8;
}
.sku-item-list .sku-item-active{
  background-color: #ed0000;
  color: #fff;
  border: 1px solid #ed0000;
}
.sku-item-unactive{
  border: 1px dashed #ccc;
  color: #999;
}
.buy-amount{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  border-top: 1px solid #e2e2e2;
  align-items: center;
}
.amount-tit{
  font-size: 28rpx;
  line-height: 100rpx;
}
.amount-btn-block{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 50rpx;
  border: 1px solid #e2e2e2;
  border-radius: 6rpx;
  font-size: 24rpx;
  color: #999;
}
.reduce-btn{
  height: 100%;
  line-height: 45rpx;
  padding: 0 24rpx;
  border-right: 1px solid #e2e2e2;
}
.amount-number{
  height: 100%;
  line-height: 50rpx;
  padding: 0 24rpx;
  color: #212121;
}
.add-btn{
  height: 100%;
  line-height: 45rpx;
  padding: 0 24rpx;
  border-left: 1px solid #e2e2e2;
}
.amount-input{
  /* border-top: 1px solid #e2e2e2;
  border-bottom: 1px solid #e2e2e2; */
  height: 100%;
  width: 100rpx;
  text-align: center;
  padding: 0;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #212121;
}
.amount-input::after{
  content: '';
  border: none;
}
.amount-input::before{
  content: '';
  border: none;
}
.no-border{
  border: none;
}

