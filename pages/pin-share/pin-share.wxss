.fast-join{
  position: relative;
  padding-bottom: 30rpx;
}
.join-box{
  width: 100%;
  height: 100%;
  padding: 22rpx 40rpx 24rpx 40rpx;
  background: rgba(254,180,180,0.10);
  border-radius: 10px;
}
.fast-title{
  font-size: 28rpx;
  color: #ED0000;
  letter-spacing: -0.34rpx;
  text-align: center;
}
.fast-content{
  position: relative;
  display: flex;
  width: 100%;
  height: 80rpx;
  align-items: center;
  margin-top: 20rpx;
}
.content-style{
  margin-bottom: 48rpx;
}
.owner-img{
  width: 80rpx;
  height: 80rpx;
}
.owner-img image{
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 1000rpx;
}
.owner-name{
  font-size: 28rpx;
  color: #212121;
  margin-left: 10rpx;
  text-align: left;
}
.group-desc{
  flex: 1;
  position: relative;
  font-size: 24rpx;
  color: #212121;
  text-align: right;
}
.lack-count{
  margin-bottom: 6rpx;
}
.lack-count .text-red{
  color: #ed0000;
}
.join-btn{
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: #ED0000;
  border-radius: 6rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  margin-left: 26rpx;
}
.more .more-icon{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -16rpx;
}
.rule{
  position: relative;
  margin-left: 24rpx;
}
.deliver-rule{
  margin-left: 34rpx;
}
.rule::before{
  content: '·';
  position: absolute;
  width: 12rpx;
  height: 36rpx;
  font-size: 24rpx;
  color: #212121;
  top: 50%;
  margin-top: -18rpx;
  text-align: center;
}
.deliver-rule::before{
  left: -22rpx;
}
.refund-rule::before{
  left: -18rpx;
}
.is-join{
  position: relative;
  display: flex;
  padding: 30rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.share-img{
  width: 200rpx;
  height: 200rpx;
  margin-right: 20rpx;
}
.share-img image {
  display: block;
  width: 100%;
  height: 100%;
}
.status-icon{
  position: absolute;
  left: 80rpx;
  top: 80rpx;
  width: 100rpx;
  height: 100rpx;
  z-index: 200;
}
.status-icon image {
  display: block;
  width: 100%;
  height: 100%;
}
